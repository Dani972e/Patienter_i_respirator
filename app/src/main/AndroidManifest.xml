<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.respirator.i.patienter.patienter_i_respirator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameActivity" />
        <activity android:name=".CommunicationActivity" />
        <activity android:name=".CallActivity" />
        <activity android:name=".TicTacToeActivity" />
        <activity android:name=".PuzzleActivity" />
        <activity android:name=".MemoryGameActivity" />
        <activity android:name=".PopBalloonsActivity" />
        <activity android:name=".SettingsActivity"></activity>
    </application>

</manifest>