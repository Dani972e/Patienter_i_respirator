<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.respirator.i.patienter.patienter_i_respirator.communication.fragments.MyAnswerFragment">

    <!-- TODO: Update blank fragment layout -->

    <TableLayout
        android:id="@+id/myAnswer_table"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TableRow
            android:id="@+id/row_1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="4">


            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/thanks_btn"
                    android:layout_width="237dp"
                    android:layout_height="146dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/button_rounded_blue"
                    android:paddingBottom="30dp"
                    android:scaleType="center"
                    app:srcCompat="@drawable/ic_thanks"
                    android:tint="@color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/iDontKnow_btn"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />

                <ImageView
                    android:id="@+id/iDontKnow_btn"
                    android:layout_width="237dp"
                    android:layout_height="146dp"
                    android:background="@drawable/button_rounded_blue"
                    android:paddingBottom="35dp"
                    android:scaleType="center"
                    app:srcCompat="@drawable/ic_i_dont_know"
                    android:tint="@color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <TextView
                    android:id="@+id/thanks_view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/tak"
                    android:textColor="@color/white"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/thanks_btn"
                    app:layout_constraintEnd_toEndOf="@+id/thanks_btn"
                    app:layout_constraintStart_toStartOf="@+id/thanks_btn"
                    app:layout_constraintTop_toTopOf="@+id/thanks_btn"
                    app:layout_constraintVertical_bias="1.0" />

                <TextView
                    android:id="@+id/iDontKnow_view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/jegVedIkke"
                    android:textColor="@color/white"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/iDontKnow_btn"
                    app:layout_constraintStart_toStartOf="@+id/iDontKnow_btn"
                    app:layout_constraintTop_toTopOf="@+id/iDontKnow_btn"
                    app:layout_constraintVertical_bias="1.0" />


            </android.support.constraint.ConstraintLayout>

        </TableRow>

        <TableRow
            android:id="@+id/row_2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="4">

            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/reset_btn"
                    android:layout_width="237dp"
                    android:layout_height="146dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginTop="12dp"
                    android:background="@drawable/button_rounded_blue"
                    android:paddingBottom="30dp"
                    android:scaleType="center"
                    app:srcCompat="@drawable/ic_settings_reset"
                    android:tint="@color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/later_btn"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />

                <ImageView
                    android:id="@+id/later_btn"
                    android:layout_width="237dp"
                    android:layout_height="146dp"
                    android:layout_marginTop="12dp"
                    android:background="@drawable/button_rounded_blue"
                    android:paddingBottom="30dp"
                    android:scaleType="center"
                    app:srcCompat="@drawable/ic_later"
                    android:tint="@color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/reset_view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/Gendan"
                    android:textColor="@color/white"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/reset_btn"
                    app:layout_constraintStart_toStartOf="@+id/reset_btn"
                    app:layout_constraintTop_toTopOf="@+id/reset_btn"
                    app:layout_constraintVertical_bias="1.0" />

                <TextView
                    android:id="@+id/later_view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/senere"
                    android:textColor="@color/white"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/later_btn"
                    app:layout_constraintEnd_toEndOf="@+id/later_btn"
                    app:layout_constraintStart_toStartOf="@+id/later_btn"
                    app:layout_constraintTop_toTopOf="@+id/later_btn"
                    app:layout_constraintVertical_bias="1.0" />
            </android.support.constraint.ConstraintLayout>

        </TableRow>

        <TableRow
            android:id="@+id/row_3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="4">




        </TableRow>

        <TableRow
            android:id="@+id/row_4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="4">


        </TableRow>

    </TableLayout>

</FrameLayout>
